{
  "author": "Yuku Takahashi <taka84u9@gmail.com>",
  "name": "jquery-textcomplete",
  "main": "dist/textcomplete",
  "version": "1.3.3",
  "homepage": "http://yuku-t.com/jquery-textcomplete",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yuku-t/jquery-textcomplete.git"
  },
  "keywords": [
    "jquery-plugin",
    "zepto-plugin"
  ],
  "bugs": {
    "url": "https://github.com/yuku-t/jquery-textcomplete/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^10.2.6",
    "exorcist": "^0.4.0",
    "jshint": "^2.8.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  },
  "scripts": {
    "lint": "jshint src/",
    "build": "browserify src/main.js -d -s tc | exorcist dist/textcomplete.js.map > dist/textcomplete.js",
    "postbuild": "uglifyjs dist/textcomplete.js -cm > dist/textcomplete.min.js",
    "watch": "watchify src/main.js -v -d -s tc -o 'exorcist dist/textcomplete.js.map > dist/textcomplete.js'"
  }
}
